{{!
    @template theme_boost/footer

    Page footer.

    Example context (json):
    {
        "output": {
            "page_doc_link": "Help and documentation",
            "supportemail": "<a href=\"#\">Contact site support</a>",
            "has_popover_links": true,
            "services_support": "Services and support",
            "login_info": "You are logged in as cute kitten",
            "moodle_release": "90210"
        }
    }
}}

<footer id="page-footer" class="footer-popover bg-white">

    <div data-region="footer-container-popover">
        <button class="btn btn-icon bg-secondary icon-no-margin btn-footer-popover" data-action="footer-popover" aria-label="{{#str}}showfooter, theme_boost{{/str}}">
            {{#pix}}e/question, core{{/pix}}
        </button>
    </div>
    <div class="footer-content-popover container" data-region="footer-content-popover">

 <div class="ast-footer astutis">
      <p class="ast-footer__ctn ast-footer__heading">
        If you have any questions please don’t hesitate to conact us.
      </p>

      <div class="ast-footer__ctn">
        <div class="ast-footer__wrp">
          <p>Telephone:</p>
          <a target="_blank" href="tel:01443849431">01443849431</a>
        </div>
        <div class="ast-footer__wrp">
          <p>Email:</p>
          <a
            target="_blank"
            href="mailto: learningadvisor@astutis.com?subject=Learner Enquiry"
            >learningadvisor@astutis.com</a
          >
        </div>
      </div>

      <div class="ast-footer-__ctn">
        <div class="ast-footer__wrp">
          <p>© 2022 Astutis Ltd.</p>
          <a target="_blank" href="https://www.astutis.com/">www.astutis.com</a>
        </div>
        <div class="ast-footer__wrp social">
          <a class="ast-footer__social"   href="https://www.facebook.com/AstutisLtd/"
            target="_blank"">
            <img
              src="/server/moodle/theme/astutis/pix/site/social-icons/facebook.svg"
              alt=""
              class="ast-footer__social__icon facebook"
            />
          </a>
          <a class="ast-footer__social"  href="https://twitter.com/Astutis"
            target="_blank">
            <img
              src="/server/moodle/theme/astutis/pix/site/social-icons/twitter.svg"
              alt=""
              class="ast-footer__social__icon twitter"
            />
          </a>
          <a class="ast-footer__social" href="https://www.youtube.com/channel/UC4-ZkGDYWzc-58m13lAGFDQ"
            target="_blank">
            <img
              src="/server/moodle/theme/astutis/pix/site/social-icons/youtube.svg"
              alt=""
              class="ast-footer__social__icon youtube"
            />
          </a>
          <a class="ast-footer__social" href="https://www.linkedin.com/company/astutis-ltd"
            target="_blank">
            <img
              src="/server/moodle/theme/astutis/pix/site/social-icons/linkedin.svg"
              alt=""
              class="ast-footer__social__icon linkedin"
            />
          </a>
        </div>
      </div>
    </div>

        <div class="footer-section border-bottom">
           
            <div class="tool_usertours-resettourcontainer">
            </div>

           
            {{{ output.standard_end_of_body_html }}}
        </div>
    
    </div>

    <div class="footer-content-debugging footer-dark bg-dark text-light">
        <div class="container-fluid footer-dark-inner">
            {{{ output.debug_footer_html }}}
        </div>
    </div>
</footer>
{{#js}}
require(['theme_boost/footer-popover'], function(FooterPopover) {
    FooterPopover.init();
});
{{/js}}
